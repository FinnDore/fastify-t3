{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "cache": false
    }
  },
  "globalEnv": ["NEXTAUTH_SECRET", "NEXTAUTH_URL", "NODE_ENV"]
}
